# AWS Credentials - For local testing, uncomment and fill these
# AWS_ACCESS_KEY_ID=your_access_key
# AWS_SECRET_ACCESS_KEY=your_secret_key
AWS_REGION=us-east-1

# Bedrock Models
EMBEDDING_MODEL_ID=amazon.titan-embed-text-v1
GENERATION_MODEL_ID=anthropic.claude-3-sonnet-20240229-v1:0

# OpenSearch Configuration
OPENSEARCH_ENDPOINT=localhost
OPENSEARCH_PORT=9200
OPENSEARCH_INDEX=rag-documents
OPENSEARCH_USE_SSL=False
OPENSEARCH_VERIFY_CERTS=False
USE_AWS_AUTH=False

# For local testing with basic auth
OPENSEARCH_USERNAME=admin
OPENSEARCH_PASSWORD=admin

# Application Settings
MAX_SEARCH_RESULTS=3
LOG_LEVEL=INFO
